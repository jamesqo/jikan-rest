{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/lumen-framework": "5.8.*",
        "vlucas/phpdotenv": "^3.3",
        "danielmewes/php-rql": "dev-master",
        "illuminate/redis": "^5.5",
        "predis/predis": "^1.1",
        "voku/anti-xss": "^4.0",
        "divineomega/cachetphp": "^0.2.0",
        "jms/serializer": "^1.13",
        "symfony/yaml": "^4.1",
        "fabpot/goutte": "3.2.3",
        "jikan-me/jikan": "^2.0",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.4",
        "flipbox/lumen-generator": "^5.6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.dist', '.env');\""
        ],
        "server:run": [
            "php -S localhost:8000 -t public"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
